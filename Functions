#include <iostream>
#include <cstdio>
using namespace std;

/*
Add `int max_of_four(int a, int b, int c, int d)` here.
*/
int max_of_four(int a, int b, int c, int d)
{
    int ans1;
    if (a>b && a>c && a>d) {
       ans1=a;
    }
    else if (b>c && b>d) {
        ans1=b;
    }
    else if (c>d) {
        ans1=c;
    }
    else {
        ans1=d;
    }
    return ans1;
}
int main() {
    int a,b,c,d;
    cin>>a>>b>>c>>d;
    int ans1=max_of_four(a, b, c, d);
    cout<<ans1;
    
    return 0;
}


